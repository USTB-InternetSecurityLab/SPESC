<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated smartContract -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="edu.ustb.sei.mde.smartContractSpecification.presentation.SmartContractSpecificationModelWizardID"
            name="%_UI_SmartContractSpecificationModelWizard_label"
            class="edu.ustb.sei.mde.smartContractSpecification.presentation.SmartContractSpecificationModelWizard"
