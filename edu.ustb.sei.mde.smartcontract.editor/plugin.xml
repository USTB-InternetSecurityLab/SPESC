   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated smartContract -->
      <editor
            id="edu.ustb.sei.mde.smartContractSpecification.presentation.SmartContractSpecificationEditorID"
            name="%_UI_SmartContractSpecificationEditor_label"
            icon="icons/full/obj16/SmartContractSpecificationModelFile.gif"
